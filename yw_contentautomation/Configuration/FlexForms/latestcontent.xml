<?xml version="1.0" encoding="utf-8"?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <el>
                    <settings.startingPoint>
                        <TCEforms>
                            <label>Starting point</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.startingPoint>
                    <settings.numberOfPages>
                        <TCEforms>
                            <label>Number of pages</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>int</eval>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.numberOfPages>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
