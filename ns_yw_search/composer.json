{
    "name": "nitsan/ns-yw-search",
    "type": "typo3-cms-extension",
    "description": "The custom extension for container",
    "keywords": [
      "TYPO3 CMS",
      "bootstrap",
      "responsive",
      "frontend",
      "theme"
    ],
    "homepage": "https://www.nitsan.in/",
    "authors": [
      {
          "name": "NITSAN Technologies Pvt Ltd",
          "email": "info@nitsan.in",
          "role": "Developer",
          "homepage": "http://www.nitsan.in/"
      }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsYwSearch\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-yw-search": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "ns_yw_search"
        }
    }
}

